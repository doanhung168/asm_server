<script src="/javascripts/bootrap5_js/bootstrap.bundle.min.js"></script>
<script src="/jquery/jquery.js"></script>
<script type="text/javascript">
    $(document).ready(() => {
        const data = '<%- data.length %>'
        if (data === '0') {
            $('#container-2').removeClass('d-none')
        } else {
            $('#container').html
            (`
                <% for(const e of data){ %>
                    <div class="col">
                        <div class="card need-hover">
                            <img src="<%= e.image %>">
                            <div class="card-body">
                                <div class="d-flex align-items-center">
                                    <i class="fa-solid fa-tag me-2"></i>
                                    <% if(e.tag[0] !== '') { %>
                                        <a href="#" class="me-2 fs-6 fst-italic"><%= '#' + e.tag[0] %></a>
                                    <% } %>
                                    <% if(e.tag[1] !== '') { %>
                                        <a href="#" class="me-2 fs-6 fst-italic"><%= '#' + e.tag[1] %></a>
                                    <% } %>
                                </div>
                                <p class="fs-3 text-dark text-uppercase text-flow-hidden" ><%= e.name %></p>
                                <p class="card-text text-flow-hidden fs-5" style="overflow-wrap: break-word"><%= e.description %></p>
                                <div class="d-flex align-items-center mt-3">

                                    <div class="d-flex align-items-center me-3">
                                        <i class="fa-solid fa-clock"></i>
                                        <span class="small fw-lighter ms-2" style="color: #929292"><%= e.updatedAt.getDate() + '/' + (e.updatedAt.getMonth() + 1) + '/' + e.updatedAt.getFullYear() + ' - ' + e.updatedAt.getHours() + ':' + e.updatedAt.getMinutes() + ':' + e.updatedAt.getSeconds() %></span>
                                    </div>

                                    <div class="d-flex align-items-center me-3">
                                        <i class="fa-solid fa-comment-dots"></i><span class="ms-2">777</span>
                                    </div>

                                    <a class="d-flex align-items-center me-3 text-decoration-none" href="#">
                                        <i class="fa-solid fa-share-nodes"></i><span class="ms-2">70000</span>
                                    </a>
                                </div>
                                <div class="d-flex justify-content-between align-items-center mt-3">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-sm btn-outline-secondary viewImage" data-value ="<%= e._id %>">View</button>
                                        <button type="button" class="btn btn-sm btn-outline-secondary editImage" data-value ="<%= e._id %>">Edit</button>
                                        <button type="button" class="btn btn-sm btn-outline-secondary deleteImage1" data-value="<%= e._id %>">Delete</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                <% } %>
            `)
        }

        $('#btnNewImage').click(() => {
            $('#newImageForm').removeClass('d-none')
        })

        $('#closeForm').click(() => {
            $('#newImageForm').addClass('d-none')
        })

        $('button[type=submit]').click((event) => {
            event.preventDefault()
            $.ajax({
                url: '/users/images',
                type: 'POST',
                data: new FormData($('#formNewImage')[0]), // The form with the file inputs.
                processData: false,
                contentType: false,
                success: (response) => {
                    if (response.result === 'redirect') {
                        window.location.replace(response.url);
                    }
                }
            }).done(function () {
                console.log("Success: Files sent!");
            }).fail(function (e) {
                console.log(e.message);
            });
        })

        $('.deleteImage1').click(function () {
            const confirmResult = confirm("Bạn chắc chắn muốn xóa ảnh này?")
            if (confirmResult) {
                $.ajax({
                    url: `/users/images/${$(this).attr('data-value')}/delete`,
                    type: 'POST',
                    success: (response) => {
                        console.log( response.data)
                        const data = response.data
                        if(data.length === 0) {
                            $('#container-2').removeClass('d-none')
                            $('#container').html('')
                        } else {
                            $('#container-2').addClass('d-none')
                            $('#container').html('')
                            for (const e of data) {
                                $('#container').append
                                (`
                                <div class="col">
                                    <div class="card need-hover">
                                        <img src="${e.image}">
                                            <div class="card-body">
                                                <div class="d-flex align-items-center">
                                                    <i class="fa-solid fa-tag me-2"></i>
                                                    <a href="#" class="me-2 fs-6 fst-italic">${e.tag[0]}</a>
                                                    <a href="#" class="me-2 fs-6 fst-italic">${e.tag[1]}</a>
                                                </div>
                                                <p class="fs-3 text-dark text-uppercase text-flow-hidden">${e.name}</p>
                                                <p class="card-text text-flow-hidden fs-5"
                                                   style="overflow-wrap: break-word">${e.description}</p>
                                                <div class="d-flex align-items-center mt-3">

                                                    <div class="d-flex align-items-center me-3">
                                                        <i class="fa-solid fa-clock"></i>
                                                        <span class="small fw-lighter ms-2"
                                                              style="color: #929292">${e.updatedAt}/span>
                                                    </div>

                                                    <div class="d-flex align-items-center me-3">
                                                        <i class="fa-solid fa-comment-dots"></i><span
                                                        class="ms-2">777</span>
                                                    </div>

                                                    <a class="d-flex align-items-center me-3 text-decoration-none"
                                                       href="#">
                                                        <i class="fa-solid fa-share-nodes"></i><span
                                                        class="ms-2">70000</span>
                                                    </a>
                                                </div>
                                                <div class="d-flex justify-content-between align-items-center mt-3">
                                                    <div class="btn-group">
                                                        <button type="button"
                                                                class="btn btn-sm btn-outline-secondary viewImage"
                                                                data-value="${e._id}">View
                                                        </button>
                                                        <button type="button"
                                                                class="btn btn-sm btn-outline-secondary editImage"
                                                                data-value="${e._id}">Edit
                                                        </button>
                                                        <button type="button"
                                                                class="btn btn-sm btn-outline-secondary deleteImage1"
                                                                data-value="${e._id}">Delete
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                    </div>
                                </div>
                            `)
                            }
                        }
                    }
                })
                    .done(() => console.log('done'))
                    .fail(() => console.log('fail'))
            }

        })

        $('.viewImage').click(function () {
                window.location.href = `/images/${$(this).attr('data-value')}`
            }
        )

        $('.editImage').click(function () {
                window.location.href = `/users/images/${$(this).attr('data-value')}/update`
            }
        )
    })
</script>

</body>
