                <button type="button" id="btnNewImage" class="btn btn-outline-success p-3 fw-bolder fs-5">CREATE NEW
                    IMAGE
                </button>
            </div>
        </section>

        <div class="container bg-white my-5 d-none" id="newImageForm">
            <form id="formNewImage" enctype="multipart/form-data" method="post">
                <div class="p-4 card shadow position-relative">
                    <i class="fa-solid fa-xmark position-absolute top-0 end-0 mx-4 my-2 p-2 bg-light"
                       id="closeForm"></i>
                    <div class="mb-3 mt-3">
                        <label for="exampleFormControlInput1" class="form-label">Tiêu đề ảnh</label>
                        <input type="text" class="form-control" id="exampleFormControlInput1" name="name">
                    </div>
                    <div class="input-group mt-3 mb-3">
                        <label for="exampleFormControlInput1" class="form-label">Gắn thẻ tag</label>
                        <div class="input-group">
                            <span class="input-group-text">Thẻ tag</span>
                            <input type="text" aria-label="First name" class="form-control" name="tag1">
                            <input type="text" aria-label="Last name" class="form-control" name="tag2">
                        </div>
                    </div>
                    <div class="mb-3">
                        <label for="exampleFormControlTextarea1" class="form-label">Nội dung mô tả ảnh</label>
                        <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"
                                  name="description"></textarea>
                    </div>
                    <div class="mb-3">
                        <label for="formFile" class="form-label">File ảnh</label>
                        <input class="form-control" type="file" id="formFile" name="image">
                    </div>
                    <div class="d-flex justify-content-evenly mt-5 mb-3">
                        <button class="btn btn-primary w-25" type="submit">Đăng ảnh</button>
                        <button class="btn btn-primary w-25" type="button" id="btnClearForm">Reset Form</button>
                    </div>
                </div>
            </form>
        </div>
        <div class="d-flex align-items-center justify-content-center d-none" id="container-2" style="min-height: 200px">
            <p class="text-center text-danger fw-bold fs-5 text-uppercase">Bạn chưa tải lên bức ảnh vào !!!</p>
        </div>

        <div class="container p-5">
            <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3" id="container"></div>
        </div>


    </main>

    <!-- Footer -->
